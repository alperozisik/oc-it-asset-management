{
    ".button": {
        "width": 256,
        "height": 65,
        "touchEnabled": true,
        "text": "Button",
        "visible": true,
        "backgroundColor": "#00A1F1",
        "alpha": 1,
        "borderColor": "rgba(0,0,0,1)",
        "borderWidth": 0,
        "textColor": "#FFFFFF",
        "textAlignment": "MIDCENTER",
        "font": {
            "size": 18,
            "bold": false,
            "italic": false,
            "family": "Lato"
        },
        "flexProps": {
            "positionType": "RELATIVE",
            "alignSelf": "AUTO"
        },
        "&-primaryColor": {
            "backgroundColor": "#FFA724"
        },
        "&-secondaryColor": {
            "backgroundColor": "#578B57"
        }
    }
}
